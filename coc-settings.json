{
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.noselect": false,
  "coc.preferences.hoverTarget": "echo",
  "coc.preferences.formatOnSaveFiletypes": [
    "Markdown",
    "coc-cfn-lint",
    "css",
    "go",
    "gomod",
    "html",
    "json",
    "markdown",
    "markdownlint",
    "python",
    "ruby",
    "sql",
    "terraform",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "python.linting.enabled": false,
  "python.jediEnabled": false,
  "python.linting.pylintPath": "/usr/local/bin/pylint",
  "python.formatting.provider": "yapf",
  "yaml.format.enable": true,
  "yaml.schemas": {
    "http://json.schemastore.org/swagger-2.0": "swagger.yaml",
    "https://jsonapi.org/schema": "swagger/**/*",
    "https://json.schemastore.org/circleciconfig": ".circleci/config.yml"
  },
  "markdownlint.onOpen": false,
  "markdownlint.onChange": false,
  "markdownlint.onSave": true,
  "codeLens.enable": true,
  "solargraph.commandPath": "/Users/james/.rbenv/gems/2.7.0/bin/solargraph",
  "solargraph.autoformat": true,
  "solargraph.useBundler": false,
  "solargraph.logLevel": "debug",
  "solargraph.rename": true,
  "solargraph.folding": true,
  "solargraph.completion": true,
  "solargraph.formatting": true,
  "solargraph.references": true,
  "solargraph.diagnostics": true,
  "solargraph.symbols": true,
  "solargraph.hover": true
}
